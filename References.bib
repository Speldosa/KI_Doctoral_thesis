	Eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/evo.12224},
	Journal = {Evolution},
	Keywords = {Body size, dosage compensation, genetic sex determination, phenotypic variability, sexual dimorphism, sexual selection},
	Number = {12},
	Pages = {3662-3668},
	Title = {The variability is in the sex chromosomes},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/evo.12224},
	Volume = {67},
