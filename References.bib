	Journal = {Developmental Psychobiology},
	Keywords = {gender differences, intrasex variability, cumulative distribution functions, birth weight, university grades},
	Number = {2},
	Pages = {198-206},
	Title = {Greater intrasex phenotype variability in males than in females is a fundamental aspect of the gender differences in humans},
