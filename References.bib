	Month = {Jul},
	Number = {1},
	Pages = {21--43},
	Title = {Trends in Gender Differences in Academic Achievement from 1960 to 1994: An Analysis of Differences in Mean, Variance, and Extreme Scores},
	Url = {https://doi.org/10.1023/A:1018873615316},
	Volume = {39},
