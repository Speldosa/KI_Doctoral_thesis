      \field{year}{2010}
      \field{pages}{39\bibrangedash 65}
      \range{pages}{27}
      \verb{doi}
      \verb 10.1002/jrsm.5
      \endverb
      \keyw{dependent effects; meta analysis; meta regression; robust standard errors}
